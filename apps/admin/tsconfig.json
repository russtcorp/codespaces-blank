{
  "extends": "@diner/ts-config/remix.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/.server/**/*.ts",
    "**/.server/**/*.tsx",
    "**/.client/**/*.ts",
    "**/.client/**/*.tsx"
  ],
  "compilerOptions": {
    "module": "ESNext",
    "types": ["@remix-run/cloudflare", "@cloudflare/workers-types", "vite/client"],
    "paths": {
      "~/*": ["./app/*"]
    }
  }
}
